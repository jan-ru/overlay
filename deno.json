{
  "tasks": {
    "start": "deno run --lock=lock.json main.ts",
    "bundle": "deno bundle main.ts bundle.js",
    "doc": "deno doc --json main.ts > docs.json",
    "serve": "deno run --allow-net --allow-read https://deno.land/std/http/file_server.ts ."
  },
  "lock": "lock.json",
  "lock-write": true
}
